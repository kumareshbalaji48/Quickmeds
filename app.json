{
  "expo": {
    "name": "quickmeds",
    "slug": "quickmeds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/quickmeds.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kumareshbalaji11.quickmeds",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to calculate proximity to the hospital."
      }
    },
    "splash": {
      "image": "./assets/images/quickmeds.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kumareshbalaji11.quickmeds",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-firebase/app",
        "@react-native-firebase/auth"
      ],
      "expo-location",
      "expo-document-picker",
      "expo-file-system",
      "@config-plugins/react-native-blob-util",
      "@config-plugins/react-native-pdf"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7f433014-ac23-4687-a079-e2960ad3d583"
      }
    },
    "owner": "kumareshbalaji11"
  }
}
